#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        string a;
        cin>>a;
        int c=0;
        for(int i=0;i<a.length();i++){
            if(a[i]=='4'){
         c++;
            
        }
        }
        cout<<c<<endl;
        
    }
	// your code goes here
	return 0;
}
