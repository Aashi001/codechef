#include <iostream>
using namespace std;



int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        int d=0;
        while(a!=0){
            d = d + a % 10;
    	a = a / 10;
    	
        }
        cout<<d<<endl;
    }
           // cout<<d<<endl;
	// your code goes here
	return 0;
}

	
