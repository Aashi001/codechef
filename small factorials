#include <iostream>
#include <boost/multiprecision/cpp_int.hpp>
using namespace std;
using namespace boost::multiprecision;

int main() {
	int t,n;
	cpp_int fact = 1;
	cin>>t;
	while(t--){
	    cin>>n;
	    fact = 1;
        for(int i = 1 ; i<=n ; i++){
            fact = fact * i;
        }
        cout << fact << endl;;
	}
	return 0;
}
