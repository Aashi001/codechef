#include <iostream>
#include<cmath>
using namespace std;

int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        
        
            int firstDigit,lastDigit,sum,c;
        
           c = log10(a);
  	
  	firstDigit = a / pow(10, c);
  	
  	lastDigit = a % 10;
  	
  	sum = firstDigit + lastDigit;
            
        
        cout<<sum<<endl;
    
    }
	// your code goes here
   return 0;
   }
