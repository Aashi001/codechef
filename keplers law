#include <stdio.h>
#include<math.h>
int main(void) {
    int n;
    scanf("%d",&n);
    while(n--){
        double t1,t2,p1,p2;
        scanf("%lf %lf %lf %lf",&t1,&t2,&p1,&p2);
    
            double t = (pow(t1, 2)/pow(p1,3));
            double p = (pow(t2, 2)/pow(p2,3));
            if(t==p){
                printf("yes\n");
            }
            else{
                printf("no\n");
            }
    }
    
	// your code goes here
	return 0;
}
